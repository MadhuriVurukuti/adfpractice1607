{
	"name": "ds_csv_parameterized",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"file": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "interviewPrep/Target/@{dataset().folder}",
					"type": "Expression"
				},
				"fileSystem": "landing"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}