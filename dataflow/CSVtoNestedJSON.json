{
	"name": "CSVtoNestedJSON",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText4",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          employeeId as string,",
				"          name as string,",
				"          department as string,",
				"          Item as string,",
				"          email as string,",
				"          phone as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(ContactInfo = @(Item_id=Item,",
				"          email_id=email,",
				"          phone_num=phone)) ~> derivedColumn1",
				"derivedColumn1 aggregate(groupBy(employeeId,",
				"          name,",
				"          department),",
				"     ContactInfo = collect(ContactInfo)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}